package package00_Utility;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;

import org.apache.poi.xssf.usermodel.XSSFWorkbook;

public class A02_ReadExcel 
{
	
	public Object data[][];
	
	public A02_ReadExcel(String filename)
	{
		File f1 = new File(System.getProperty("user.dir") + "//TestData//Data.xlsx");
		FileInputStream fs;
		XSSFWorkbook wb;
		try {
			fs = new FileInputStream(f1);
			wb = new XSSFWorkbook(fs);
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}

		int rows = wb.getSheet("userdata").getPhysicalNumberOfRows();
		int col = wb.getSheet("userdata").getRow(0).getPhysicalNumberOfCells();

		Object data[][] = new Object[rows-1][col];                    

		for (int i = 1; i < rows; i++) 
		{
			for (int j = 0; j < col; j++) 
			{
				data[i-1][j] = wb.getSheet("userdata").getRow(i).getCell(j).getStringCellValue();
				System.out.print(data[i-1][j]+" ");
			}
		}
	}
	
	public void getData()
	{
		return 
	}

}
